# Copy this file to `.env` and fill in values.
# `.env` is gitignored.

# Cozi iCalendar feed URL (optional)
COZI_ICS_URL=

# Authentication (recommended if accessing remotely)
SFO_PASSWORD=
SFO_SESSION_SECRET=
SFO_USERNAME=
SFO_API_TOKEN=
SFO_HTTPS_ONLY=
SFO_SESSION_SAMESITE=lax
SFO_SESSION_MAX_AGE=

# Coach (local LLM via Ollama; optional)
SFO_LLM_PROVIDER=auto
SFO_OLLAMA_URL=http://localhost:11434
SFO_OLLAMA_MODEL=llama3.1:8b
SFO_LLM_TIMEOUT=15
SFO_COACH_HISTORY_LIMIT=120
